{
  "extends": ["@rocketseat/eslint-config/react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 80,
        "tabWidth": 2,
        "singleQuote": true,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "semi": false,
        "endOfLine": "auto"
      }
    ]
  }
}
